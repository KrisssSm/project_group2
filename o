№2
нод
a, b = map(int,input().split()
while a != 0 and b != 0:

    if a > b:
        a = a % b
    else:
        b = b % a
 
ans = a + b
print(ans)

нок
a,b =map(int,input().split())
m=max(b,a)
while True:
    if m%b==0 and m%a==0:
        print(m)
        break
    else:
        m=m+1  


№4
а. n = int(input())
sum=0
for i in range(1, n+1):
    sum=sum+i**3
print(sum)
б. n = int(input())
first_factorial=1
sum=0
for i in range(1, n+1):
    first_factorial = first_factorial * i
    sum=sum+first_factorial
print(sum)


№5
n = int(input("Введите целое число: ")) 
f0 = 0                                              #f0 число Фибоначчи (по условию)    
f1 = 1                                              #f1 число Фибоначчи (по условию)
for i in range (1, n):                              #берем до n, а не n+1, так как последовательность начинается с нулевого элемента
    fn = f0 + f1                                    #рассчитываем последующее число Фибоначчи
    f0 = f1                                         #присваиваем новое значения для дальнейшей работы, так как используем формулы с тремя переменными  (fn = f0 + f1)
    f1 = fn                                         #аналогично   
print("n-ое число Фибоначчи:", f1)                  #выводит n-ное число

№6
n = int(input("Введите натуральное число: "))  
full_sum = (n * (n + 1)) // 2                       #найдем сумму чисел от 1 до n включительно по формуле арифметической прогрессии       
current_sum = 0                                     #запустим счетчик, считывающий текущую сумму (сумма без пропущенного числа)
for i in range(n - 1):
    num = int(input())                              #вводим числа (n-1)
    current_sum += num                              #перебираем все введенные числа, каждое прибавляем к текущей сумме
missing = full_sum - current_sum                    #находим пропущенное число
print("Пропущенное число:", missing)

